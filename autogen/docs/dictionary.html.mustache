    <div class="dict_entry" id="clear-output">
      <h3>
        <a>clear-output<span class="since">1.0</span></a>
      </h3>
      <h4>
        <span class="prim_example">clear-output</span>
        <img alt="Observer Command" src="images/observer.gif">
      </h4>
